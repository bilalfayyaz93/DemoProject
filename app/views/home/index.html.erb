<h1>Home#index</h1>
<p>Find me in app/views/home/index.html.erb</p>
<style>
img {
  border-radius: 50%;
}
</style>
<p><%=current_user%></p>
<%if current_user%>
  <p><%=current_user.avator.signed_id%></p>

  <%if current_user.avator.attached?%>
    <%= image_tag(current_user.avator,height: 200, width: 200, class: 'img' )%>
  <%end%>
  <%=link_to "edit" , edit_user_registration_path %>
  <%=link_to "Sign out" , destroy_user_session_path, method: :delete, data: {confirm: "are you sure?"}%>
<%end%>
