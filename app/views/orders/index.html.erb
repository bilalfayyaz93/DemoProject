<h1>Orders</h1>

<table class="table">
  <thead>
    <tr>
      <th>User</th>
      <th>Coupen</th>
      <th>Checkout date</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @orders.each do |order| %>
      <tr>
        <td><%= order.user_id %></td>
        <td><%= order.coupen_id %></td>
        <td><%= order.created_at %></td>
        <td><%= link_to 'Show', order %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

